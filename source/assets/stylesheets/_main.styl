@import 'nib'

color_1 = #7aa6da
color_2 = #66cccc
color_3 = #99cc99
color_4 = #f99157
color_5 = #f2777a
neutral_1 = #cccccc
neutral_2 = #999999
neutral_3 = #515151
neutral_4 = #393939
neutral_5 = #2d2d2d

black = neutral_5
gray = neutral_3
white = color_3


html,body
  height 100%
  width 100%

body
  background black
  text-align center
  &:before
    content: ''
    display inline-block
    height 100%
    vertical-align middle
    letter-spacing 0px

#wrap
  display inline-block
  vertical-align middle
